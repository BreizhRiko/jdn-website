<form method="post" action="{{ path('table_delete', {'id': table.id}) }}" onsubmit="return confirm('Êtes vous sûr de vouloir supprimer ce table ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ table.id) }}">
    <button class="btn btn-sm btn-warning float-end">Supprimer</button>
</form>
